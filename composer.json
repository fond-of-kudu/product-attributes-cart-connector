{
    "name": "fond-of-spryker/product-attributes-cart-connector",
    "description": "n/a",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "spryker/product": "^6.0.0",
        "spryker/cart-extension": "^4.2.0"
    },
    "require-dev": {
      "fond-of-codeception/spryker": "*",
      "spryker/code-sniffer": "*",
      "spryker-sdk/phpstan-spryker": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
      "psr-4": {
        "FondOfKudu\\": "src/FondOfKudu/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "FondOfKudu\\": "tests/FondOfKudu/",
        "Generated\\": "src/Generated/"
      }
    },
    "config": {
      "sort-packages": true,
      "allow-plugins": {
        "dealerdirect/phpcodesniffer-composer-installer": true
      }
    }
}
